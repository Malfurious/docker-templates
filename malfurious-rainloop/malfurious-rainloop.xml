<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>True</Beta>
  <Category>Other:Tools</Category>
  <Date></Date>
  <Name>malfurious-rainloop</Name>
  <Support></Support>
  <Description>
	Rainloop is a versatile webmail application.
	[b]To enable the ability to save Contacts, you will need to set it up manually in the webgui.[/b]
	[b]Please leave "--name postfixadmin" in the Extra Parameters box.[/b]
  </Description>
  <Overview>
  </Overview>
  <Project>https://www.rainloop.net/</Project>
  <Registry>https://registry.hub.docker.com/u/malfurious/rainloop/</Registry>
  <Repository>malfurious/rainloop</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
      <Publish>
      <Port>
        <HostPort></HostPort>
        <ContainerPort>8888</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
   </Publish>
  </Networking>
  <Data>
  	<HostDir>/rainloop/data</HostDir>
      <ContainerDir>/mnt/docker/rainloop</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
<WebUI></WebUI>
  <Icon>https://raw.githubusercontent.com/Malfurious/docker-templates/master/images/RainloopIcon.png</Icon>
  <ExtraParams></ExtraParams> 
<DonateText></DonateText>
<DonateLink></DonateLink>
<DonateImg></DonateImg>
</Container>